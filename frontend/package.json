{
  "name": "rewine-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:mock": "VITE_MOCK_API=true vite",
    "build": "vue-tsc && vite build",
    "build:prod": "VITE_APP_ENV=production vue-tsc && vite build",
    "build:docker": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:setup": "playwright install",
    "msw:init": "npx msw init public/ --save",
    "docker:build": "docker build -t rewine-frontend .",
    "docker:run": "docker run -p 8080:80 rewine-frontend",
    "docker:dev": "docker build -t rewine-frontend . && docker run -p 8080:80 rewine-frontend"
  },
  "dependencies": {
    "@vueuse/core": "^12.4.0",
    "axios": "^1.7.9",
    "pinia": "^2.3.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.0.1",
    "vue-router": "^4.5.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@playwright/test": "^1.49.1",
    "@types/node": "^22.10.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^26.0.0",
    "msw": "^2.12.7",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.7",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}